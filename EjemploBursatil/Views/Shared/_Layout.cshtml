<!DOCTYPE html>
<html lang="es">
    <head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width">
        <title>@ViewBag.Title - APLICACION BURSATIL</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
         <script src="~/Scripts/canvasjs.min.js"></script>

        <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" href="~/Content/justified-nav.css" />
         <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/canvasjs.min.js"></script>

    </head>
    <body>
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <p class="site-title">
                        <img src="~/Images/logo.jpg" style="width:40px;height:40px" />
                    </p>
                </div>
                <div class="float-right masterhead">
                    <nav>
                        <ul id="menu" class="">
                            <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                             <li>@Html.ActionLink("Ejemplo", "Example", "Home")</li>
                            <li>@Html.ActionLink("Contacto", "Contact", "Home")</li>
                        </ul>
                        <section id="login">
                            @Html.Partial("_LoginPartial")
                        </section>
                    </nav>
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
            @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET MVC</p>
                </div>
            </div>
        </footer>

      @*  @Scripts.Render("~/bundles/jquery")*@
        @*@RenderSection("scripts", required: false)*@
    </body>
</html>
